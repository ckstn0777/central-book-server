{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "^7.13.0",
    "aws-sdk": "^2.935.0",
    "axios": "^0.21.1",
    "crypto-js": "^4.0.0",
    "fastify": "^3.17.0",
    "fastify-compress": "^3.6.0",
    "fastify-cookie": "^5.3.1",
    "fastify-cors": "^6.0.2",
    "fastify-multipart": "^4.0.6",
    "fastify-static": "^4.2.2",
    "global": "^4.4.0",
    "googleapis": "^77.0.0",
    "joi": "^17.4.1",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.31",
    "mysql": "^2.18.1",
    "pino": "^6.11.3",
    "pino-pretty": "^5.0.2",
    "pm2": "^5.1.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.28.3",
    "typeorm": "^0.2.34",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.0.1",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^15.12.2",
    "@types/pino": "^6.3.9",
    "@types/sharp": "^0.28.3",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "json-schema-to-typescript": "^10.1.4",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  },
  "scripts": {
    "start": "yarn forever start -c 'yarn run start:dev' ./",
    "start:dev": "cross-env NODE_PATH=src ts-node-dev --respawn --transpile-only ./src/index.ts | pino-pretty",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "db:sync": "yarn typeorm schema:sync",
    "search": "ts-node-dev --respawn --transpile-only ./src/lib/search/index.ts",
    "compile-schemas": "json2ts -i ./src/schema -o ./src/types"
  }
}
